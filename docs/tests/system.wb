TimerStart()

string LINUX = "Linux"
string WINDOWS = "Windows"

let os = GetOs()
if os = OS_LINUX then
	Show(LINUX $ NEW_LINE)
if os = OS_WINDOWS then
	Show(WINDOWS $ NEW_LINE)

string SECOND_UNIT = " ns"

let elapsed_time = TimerGetElapsedTimeInUs()
Show(ToString(elapsed_time) $ SECOND_UNIT $ NEW_LINE)
